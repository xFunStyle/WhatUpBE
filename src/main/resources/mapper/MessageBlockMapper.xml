<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="io.spring.infrastructure.mybatis.mapper.MessageBlockMapper">
    <insert id="saveBlock">
        insert into messageBlocks (blockId, messageId) values(
        #{blockId},
        #{messageId}
        )
    </insert>

</mapper>